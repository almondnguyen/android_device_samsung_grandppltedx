/sys/devices/virtual/sec/sec_touchkey/keypad_enable     u:object_r:sysfs_display:s0

# Display
/sys/devices/virtual/lcd/panel/power_reduce             u:object_r:sysfs_display:s0

# Macloader
/system/bin/macloader                                   u:object_r:macloader_exec:s0

