# Bluetooth
/dev/btlock                                             u:object_r:bluetooth_device:s0
/dev/rfkill                                             u:object_r:bluetooth_device:s0
/efs/bluetooth(/.*)?                                    u:object_r:bluetooth_efs_file:s0

# Camera
